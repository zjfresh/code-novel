{
	"name": "code-novel",
	"displayName": "Code Novel",
	"description": "代码注释小说",
	"version": "0.0.6",
	"publisher": "mathon",
	"icon": "finish.png",
	"engines": {
		"vscode": "^1.34.0"
	},
	"keywords": [
		"摸鱼",
		"看书",
		"神器",
		"代码看书"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"keybindings": [
			{
				"command": "extension.openNovel",
				"key": "ctrl+alt+,",
				"mac": "cmd+alt+,"
			},
			{
				"command": "extension.nextpage",
				"key": "windows+alt+.",
				"mac": "ctrl+alt+."
			},
			{
				"command": "extension.prevpage",
				"key": "windows+alt+,",
				"mac": "ctrl+alt+,"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"eslint": "^7.5.0",
		"typescript": "^3.3.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/iyim/code-novel"
	}
}
